{
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "module": "esnext",
    "target": "es6",
    "lib": [
      "dom",
      "es2015"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
  },
  "extends": "expo/tsconfig.base",
  "include": [
    "src",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
 
}
